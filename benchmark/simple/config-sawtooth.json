{
  "blockchain": {
    "type": "sawtooth",
    "config": "benchmark/simple/sawtooth.json"
  },
  "test": {
    "clients": {
      "type": "local",
      "number": 1
    },
    "rounds": [
      {
        "label": "open",
        "txNumber": [
          10
        ],
        "rateControl": [
          {
            "type": "fixed-rate",
            "opts": {
              "tps": 10
            }
          }
        ],
        "arguments": {
          "money": ""
        },
        "callback": "benchmark/simple/open.js"
      },
      {
        "label": "query",
        "txNumber": [
          10
        ],
        "rateControl": [
          {
            "type": "fixed-rate",
            "opts": {
              "tps": 10
            }
          }
        ],
        "callback": "benchmark/simple/query.js"
      }
    ]
  },
  "monitor": {
    "type": "docker",
    "docker": {
      "name": [
        "all"
      ]
    },
    "interval": 1
  }
}